<view class="container999"><view class="title">创建合格证</view><view class="line"><view class="lineLeft">食用农产品名称</view><view class="lineRight"><input class="input" data-name="name" placeholder-class="plaClass" placeholder="食用农产品名称" data-event-opts="{{[['input',[['change',['$event']]]]]}}" value="{{form.name}}" bindinput="__e"/></view></view><view class="line"><view class="lineLeft">重量(数量)</view><view class="lineRight"><picker value="{{form.region}}" custom-item="{{customItem}}" data-event-opts="{{[['change',[['change',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+form.region[0]+''}}</view></picker><view class="tips tri"></view></view></view><view class="line"><view class="lineLeft">手机号</view><view class="lineRight"><input class="input" data-name="phone" placeholder-class="plaClass" placeholder="请输入手机号" data-event-opts="{{[['input',[['change',['$event']]]]]}}" value="{{form.phone}}" bindinput="__e"/></view></view><view class="line"><view class="lineLeft">身份证</view><view class="lineRight"><input class="input" data-name="id" placeholder-class="plaClass" placeholder="请输入身份证" data-event-opts="{{[['input',[['change',['$event']]]]]}}" value="{{form.id}}" bindinput="__e"/></view></view><view class="line"><view class="lineLeft">地址</view><view class="lineRight"><picker mode="region" data-name="region" value="{{form.region}}" custom-item="{{customItem}}" data-event-opts="{{[['change',[['change',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+form.region[0]+''}}</view></picker><view class="tips tri"></view></view></view><view class="line"><view class="lineLeft">备注</view><view class="lineRight"><input class="input" data-name="remarks" placeholder-class="plaClass" placeholder="备注" data-event-opts="{{[['input',[['change',['$event']]]]]}}" value="{{form.remarks}}" bindinput="__e"/><view class="tips">选填</view></view></view><view data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" class="buttonBox" bindtap="__e">提交</view></view>