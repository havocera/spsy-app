<view><button data-event-opts="{{[['tap',[['searchBle',['$event']]]]]}}" bindtap="__e">搜索蓝牙</button><block qa:for="{{devices}}" qa:for-item="item" qa:for-index="index" qa:key="index"><view style="margin-top:30rpx;"><button data-event-opts="{{[['tap',[['onConn',['$0'],[[['devices','',index]]]]]]]}}" style="width:400rpx;color:#0081FF;" bindtap="__e">{{item.name}}</button></view></block><input type="text" value="{{url}}" name="url" placeholder="请输入二维码url" data-event-opts="{{[['input',[['__set_model',['','url','$event',[]]]]]]}}" bindinput="__e"/><input type="text" value="{{txt}}" name="url" placeholder="请输入文字" data-event-opts="{{[['input',[['__set_model',['','txt','$event',[]]]]]]}}" bindinput="__e"/><button data-event-opts="{{[['tap',[['senBleLabel']]]]}}" style="margin-top:100rpx;" bindtap="__e">标签打印cpcl</button><textarea auto-height="{{true}}" placeholder-style="color:#F76260" placeholder="请输入票据信息" data-event-opts="{{[['input',[['__set_model',['','piaojuText','$event',[]]]]]]}}" value="{{piaojuText}}" bindinput="__e"></textarea><button data-event-opts="{{[['tap',[['senBleLabel2']]]]}}" style="margin-top:100rpx;" bindtap="__e">标签打印zpl</button></view>